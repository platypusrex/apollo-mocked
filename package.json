{
  "name": "apollo-mocked",
  "version": "2.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/dts/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:ts\"",
    "build:cjs": "swc src -d dist/cjs --config-file .swcrc -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm --config-file .swcrc -C module.type=es6",
    "build:ts": "tsc --project tsconfig.type.json",
    "clean": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "check:types": "tsc --noEmit",
    "test": "jest test --coverage",
    "lint": "eslint --cache '{src,test}/**.{ts,tsx,js,md}'",
    "lint:fix": "eslint '{src,test}/**.{ts,tsx,js,md}' --fix",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "peerDependencies": {
    "@apollo/client": ">=3.3.1",
    "graphql": ">=16.*",
    "react": ">=16"
  },
  "devDependencies": {
    "@apollo/client": "3.3.1",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "1.2.3",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.58",
    "@swc/jest": "^0.2.26",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^29.4.0",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "concurrently": "^8.0.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@graphql-tools/mock": "^8.5.2"
  },
  "resolutions": {
    "graphql": "16.6.0"
  }
}
