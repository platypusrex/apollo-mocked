{
  "name": "apollo-mocked",
  "version": "1.0.1",
  "main": "dist/index.js",
  "module": "dist/apollo-mocked.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jest-environment-jsdom-sixteen --coverage",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "peerDependencies": {
    "@apollo/client": ">=3.3.1",
    "graphql": ">=15.*",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@apollo/client": "3.3.1",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "1.2.3",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "eslint-plugin-prettier": "3.1.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.13.1",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@graphql-tools/mock": "^8.5.2"
  }
}
